rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }
    match /art/{docid}{
      allow read: if true;
      allow write: if false
    }
    match /application/{docid}{
      allow read: if true;
      allow write: if false
    }
  }
}